<% if (locals.messages) { %>
    <div class="messages-container">
        <ul class="messages">
          <% Object.keys(messages).forEach((key) => { %> <%
          messages[key].forEach((message) => { %>
          <li class="<%= key %>"><%= message %></li>
          <% }) %> <% }) %>
        </ul>
    </div>
<% } %>